invl main()
    array[3] target = [5, 2, 3]
    array[3] key = [220, 159, 99]
    array[3] temp = [0, 0, 0]

    print(target)
    print(key)
    print(temp)
    with call crypt(target, key, temp)

invl crypt(array[3] target, array[3] key, array[3] temp)
    local int index = 0
        from index = 0
        do skip
        loop
            temp[index] += target[index] ^ key[index]
            index += 1
        until index = 3
    delocal int index = 3
    with temp <=> target
